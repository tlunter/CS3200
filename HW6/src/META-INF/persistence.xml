<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="HW6">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>HW6.Site</class>
        <class>HW6.Tower</class>
        <class>HW6.Equipment</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost/cs3200_hw6"/>
            <property name="javax.persistence.jdbc.driver" value="org.mariadb.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.user" value="cs3200_hw6"/>
